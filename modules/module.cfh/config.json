    },
    
    "queries":{
    	
    	"GET_KARMA":"SELECT * FROM `karma` WHERE `serverId` = ? AND `carrier` = ?",
    	"SET_KARMA":"INSERT INTO `karma` (`serverId`, `carrier`, `karma`, `log`) VALUES (?, ?, ?, ?) ON DUPLICATE KEY UPDATE `karma`=(VALUES(`karma`)+`karma`), `log`=CONCAT(`log`,VALUES(`log`))"
    	
    }
    
